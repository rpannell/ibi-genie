{
  "name": "ApplicationFrameworkManagement",
  "version": "1.1.71",
  "description": "Application Framework Management",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "release": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "publish": {
    "provider": "github",
    "repo": "ibi-genie",
    "private": false
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.4",
    "electron-builder": "^22.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rpannell/ibi-genie.git"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^3.4.1",
    "bootstrap-table": "^1.18.0",
    "commander": "^2.20.3",
    "electron-config": "^2.0.0",
    "electron-ipc": "^0.3.0",
    "electron-json-storage": "^4.2.0",
    "electron-log": "^2.2.17",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.18.3",
    "jquery": "^3.5.1",
    "jquery-migrate": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.19.2",
    "jsonfile": "^3.0.1",
    "materialize-css": "^0.100.2",
    "moment": "^2.29.1",
    "mssql": "^6.2.3",
    "q": "^1.5.1",
    "redis": "^3.0.2",
    "winston": "^3.3.3",
    "yeoman-environment": "^2.10.3",
    "yeoman-generator": "^4.12.0",
    "yo": "^3.1.1",
    "generator-ibi-appframework": "1.1.40"
  },
  "build": {
    "appId": "apps.cloud.interlinebrands.com",
    "buildDependenciesFromSource": true,
    "win": {
      "publish": [
        "github"
      ],
      "target": "nsis",
      "icon": "build/iconA.ico"
    },
    "nsis": {
      "include": "build/installer-globals.nsi"
    },
    "extraFiles": [
      {
        "filter": [
          "*.tgz",
          "*.bat"
        ]
      }
    ]
  }
}
